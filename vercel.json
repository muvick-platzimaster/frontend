{
   "version": 2,
   "builds": [
      {
         "src": "package.json",
         "use": "@vercel/static-build"
      }
   ],
   "routes": [
      {
         "handle": "filesystem"
      },
      {
         "src": "/.*",
         "dest": "/"
      },
      {
         "src": "(.*).js",
         "dest": "$1.js"
      },
      {
         "src": "(.*).json",
         "dest": "$1.json"
      }
   ]
}
